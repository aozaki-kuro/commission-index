{
  "name": "nsfw-commission-neo",
  "version": "1.3.0",
  "private": true,
  "author": "Crystallize",
  "scripts": {
    "build": "bun run pre-build ; next build",
    "clean": "rm -rf .next node_modules out .wrangler ; bun i",
    "deploy": "bun run prod ; wrangler deploy",
    "dev": "bun run pre-build ; next dev --turbo",
    "lint": "eslint --fix",
    "pre-build": "bun run scripts/convert.ts ; bun run scripts/imageImport.ts",
    "prepare": "husky",
    "prod": "bun run pre-build ; next build",
    "start": "next start"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,tsx}": [
      "bun lint"
    ],
    "*.{json,css,md,mdx,jsonc}": [
      "bun prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.9",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.3.1",
    "fuse.js": "^7.1.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rss-parser": "^3.13.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/eslint__eslintrc": "^3.3.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "wrangler": "^4.65.0"
  }
}
